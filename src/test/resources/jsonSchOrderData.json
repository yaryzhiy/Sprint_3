{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "required": [
        "orders",
        "pageInfo",
        "availableStations"
    ],
    "properties": {
        "orders": {
            "$id": "#/properties/orders",
            "type": "array",
            "additionalItems": true,
            "items": {
                "$id": "#/properties/orders/items",
                "anyOf": [
                    {
                        "$id": "#/properties/orders/items/anyOf/0",
                        "type": "object",
                        "required": [
                            "id",
                            "courierId",
                            "firstName",
                            "lastName",
                            "address",
                            "metroStation",
                            "phone",
                            "rentTime",
                            "deliveryDate",
                            "track",
                            "color",
                            "comment",
                            "createdAt",
                            "updatedAt",
                            "status"
                        ],
                        "properties": {
                            "id": {
                                "$id": "#/properties/orders/items/anyOf/0/properties/id",
                                "type": "integer"
                            },
                            "courierId": {
                                "$id": "#/properties/orders/items/anyOf/0/properties/courierId",
                                "type": ["number", "null"]
                            },
                            "firstName": {
                                "$id": "#/properties/orders/items/anyOf/0/properties/firstName",
                                "type": "string"
                            },
                            "lastName": {
                                "$id": "#/properties/orders/items/anyOf/0/properties/lastName",
                                "type": "string"
                            },
                            "address": {
                                "$id": "#/properties/orders/items/anyOf/0/properties/address",
                                "type": "string"
                            },
                            "metroStation": {
                                "$id": "#/properties/orders/items/anyOf/0/properties/metroStation",
                                "type": "string"
                            },
                            "phone": {
                                "$id": "#/properties/orders/items/anyOf/0/properties/phone",
                                "type": "string"
                            },
                            "rentTime": {
                                "$id": "#/properties/orders/items/anyOf/0/properties/rentTime",
                                "type": "integer"
                            },
                            "deliveryDate": {
                                "$id": "#/properties/orders/items/anyOf/0/properties/deliveryDate",
                                "type": "string"
                            },
                            "track": {
                                "$id": "#/properties/orders/items/anyOf/0/properties/track",
                                "type": "integer"
                            },
                            "color": {
                                "$id": "#/properties/orders/items/anyOf/0/properties/color",
                                "type": ["array", "null"],
                                "additionalItems": true,
                                "items": {
                                    "$id": "#/properties/orders/items/anyOf/0/properties/color/items",
                                    "anyOf": [
                                        {
                                            "$id": "#/properties/orders/items/anyOf/0/properties/color/items/anyOf/0",
                                            "type": "string"
                                        }
                                    ]
                                }
                            },
                            "comment": {
                                "$id": "#/properties/orders/items/anyOf/0/properties/comment",
                                "type": "string"
                            },
                            "createdAt": {
                                "$id": "#/properties/orders/items/anyOf/0/properties/createdAt",
                                "type": "string"
                            },
                            "updatedAt": {
                                "$id": "#/properties/orders/items/anyOf/0/properties/updatedAt",
                                "type": "string"
                            },
                            "status": {
                                "$id": "#/properties/orders/items/anyOf/0/properties/status",
                                "type": "integer"
                            }
                        },
                        "additionalProperties": true
                    }
                ]
            }
        },
        "pageInfo": {
            "$id": "#/properties/pageInfo",
            "type": "object",
            "required": [
                "page",
                "total",
                "limit"
            ],
            "properties": {
                "page": {
                    "$id": "#/properties/pageInfo/properties/page",
                    "type": "integer"
                },
                "total": {
                    "$id": "#/properties/pageInfo/properties/total",
                    "type": "integer"
                },
                "limit": {
                    "$id": "#/properties/pageInfo/properties/limit",
                    "type": "integer"
                }
            },
            "additionalProperties": true
        },
        "availableStations": {
            "$id": "#/properties/availableStations",
            "type": "array",
            "additionalItems": true,
            "items": {
                "$id": "#/properties/availableStations/items",
                "anyOf": [
                    {
                        "$id": "#/properties/availableStations/items/anyOf/0",
                        "type": "object",
                        "required": [
                            "name",
                            "number",
                            "color"
                        ],
                        "properties": {
                            "name": {
                                "$id": "#/properties/availableStations/items/anyOf/0/properties/name",
                                "type": "string"
                            },
                            "number": {
                                "$id": "#/properties/availableStations/items/anyOf/0/properties/number",
                                "type": "string"
                            },
                            "color": {
                                "$id": "#/properties/availableStations/items/anyOf/0/properties/color",
                                "type": "string"
                            }
                        },
                        "additionalProperties": true
                    }
                ]
            }
        }
    },
    "additionalProperties": true
}